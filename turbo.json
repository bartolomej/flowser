{
  "npmClient": "yarn",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "compile": {
      "dependsOn": ["^build", "^compile"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
